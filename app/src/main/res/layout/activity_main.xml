<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">


    <View
        android:id="@+id/rectangle4"
        android:layout_width="413dp"
        android:layout_height="96dp"
        android:layout_marginTop="16dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <TextView
        android:id="@+id/title"
        android:layout_width="420dp"
        android:layout_height="78dp"
        android:background="#000000"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="KIPPIE"
        android:textColor="@color/kippie"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Second rectangle (Black rectangle) -->


    <TextView
        android:id="@+id/textView2"
        android:layout_width="154dp"
        android:layout_height="56dp"
        android:fontFamily="monospace"
        android:text="Acties"
        android:textColor="@color/black"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115" />

    <ImageButton
        android:id="@+id/eelHuisje"
        android:layout_width="112dp"
        android:layout_height="99dp"
        android:layout_marginTop="6dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/icons8_home_32" />

    <ImageButton
        android:id="@+id/witmenuhome"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/witkarretjehome"
        app:layout_constraintStart_toEndOf="@+id/eelHuisje"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:srcCompat="@drawable/icons8_restaurant_menu_64__2_" />

    <ImageButton
        android:id="@+id/witkarretjehome"
        android:layout_width="112dp"
        android:layout_height="96dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/geelmenu"
        app:layout_constraintTop_toBottomOf="@+id/rectangle2"
        app:srcCompat="@drawable/icons8_shopping_cart_52__2_" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="112dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="177dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="136dp"
            android:layout_height="358dp"
            android:layout_marginEnd="222dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.638"
            app:srcCompat="@drawable/redbull_schaduw_800x800" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="228dp"
            android:layout_height="270dp"
            android:layout_marginStart="91dp"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.744"
            app:srcCompat="@drawable/piripirisaus_schaduw_800x800" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="252dp"
            android:layout_height="258dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.913"
            app:srcCompat="@drawable/broodje_kipburger_schaduw_800x800" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="61dp"
            android:layout_height="62dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.591"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.383"
            app:srcCompat="@drawable/icons8_sale_100" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="85dp"
            android:layout_height="48dp"
            android:text="â‚¬7,99"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.791"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.35" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="338dp"
            android:layout_height="59dp"
            android:fontFamily="monospace"
            android:text="Ziekenhuis deal"
            android:textColor="@color/black"
            android:textSize="37sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.563"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/screenshot_2024_11_04_110212" />

        <!-- Floating action button for add alarm -->
        <!-- Make sure that you are constraining this
             button to the parent button -->

        <!-- Action name text for the add alarm button -->
        <!-- Make sure that you are constraining this Text to
             the add Alarm FAB button -->

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/FloatingActionButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="4dp"
            android:foreground="@drawable/icons8_settings_150"
            app:backgroundTint="@color/kippie"
            app:fabSize="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/add_alarm_fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="4dp"
            android:foreground="@drawable/icons8_map_90"
            app:backgroundTint="@color/kippie"
            app:fabSize="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/add_person_fab"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/add_alarm_action_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/add_alarm_fab"
            app:layout_constraintEnd_toStartOf="@+id/add_alarm_fab"
            app:layout_constraintTop_toTopOf="@+id/add_alarm_fab" />

        <!-- Floating action button for add person -->
        <!-- Make sure that you are constraining this
             button to the add Alarm FAB button -->

        <!-- Action name text for the add person button -->
        <!-- Make sure that you are constraining this Text
             to the add Person FAB button -->

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/add_person_fab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="4dp"
            android:foreground="@drawable/icons8_about_50"
            app:backgroundTint="@color/kippie"
            app:fabSize="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/FloatingActionButton" />

        <TextView
            android:id="@+id/add_person_action_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/add_person_fab"
            app:layout_constraintEnd_toStartOf="@+id/add_person_fab"
            app:layout_constraintTop_toTopOf="@+id/add_person_fab" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
