<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/title"
        android:layout_width="420dp"
        android:layout_height="78dp"
        android:background="#000000"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Kippie"
        android:textColor="@color/kippie"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle2"
        android:layout_width="407dp"
        android:layout_height="612dp"
        android:background="#ffffff"
        app:layout_constraintBottom_toTopOf="@+id/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintVertical_bias="0.0" />

    <!-- Second rectangle (Black rectangle) -->
    <View
        android:id="@+id/rectangle"
        android:layout_width="413dp"
        android:layout_height="71dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="scherm2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/withuisjemenu"
        android:layout_width="111dp"
        android:layout_height="101dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/screenshot_2024_11_04_110322"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="689dp" />

    <ImageButton
        android:id="@+id/geelmenu"
        android:layout_width="96dp"
        android:layout_height="84dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/screenshot_2024_11_04_110251"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="694dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="101dp"
        android:layout_height="97dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/screenshot_2024_11_04_110229"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="689dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
