<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".scherm2">

    <TextView
        android:id="@+id/title"
        android:layout_width="420dp"
        android:layout_height="78dp"
        android:background="#000000"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="KIPPIE"
        android:textColor="@color/kippie"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle2"
        android:layout_width="410dp"
        android:layout_height="585dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <ScrollView
        android:layout_width="374dp"
        android:layout_height="488dp"
        android:gravity="clip_horizontal|center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.641">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    app:srcCompat="@drawable/minisateballenpanxxl" />

                <Button
                    android:id="@+id/hapjespannen"
                    android:layout_width="224dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/white"
                    android:fontFamily="monospace"
                    android:text="Hapjes pannen"
                    android:textColor="@color/black"
                    android:textSize="30dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView90"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    app:srcCompat="@drawable/kippievegamaaltijdpan" />

                <Button
                    android:id="@+id/Maaltijdpannen"
                    android:layout_width="224dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/white"
                    android:fontFamily="monospace"
                    android:text="Maaltijd pannen"
                    android:textColor="@color/black"
                    android:textSize="30dp" />
                />


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="50dp"
                    android:layout_height="150dp"
                    app:srcCompat="@drawable/satepanpiri" />

                <Button
                    android:id="@+id/Vleespannen"
                    android:layout_width="224dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/white"
                    android:fontFamily="monospace"
                    android:text="Vlees pannen"
                    android:textColor="@color/black"
                    android:textSize="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="50dp"
                    android:layout_height="150dp"
                    app:srcCompat="@drawable/aardappelpan" />

                <Button
                    android:id="@+id/Vegapannen"
                    android:layout_width="224dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/white"
                    android:fontFamily="monospace"
                    android:text="Vega pannen"
                    android:textColor="@color/black"
                    android:textSize="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    app:srcCompat="@drawable/knakpanxxl" />

                <Button
                    android:id="@+id/XXLpannen"
                    android:layout_width="224dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/white"
                    android:fontFamily="monospace"
                    android:text="XXL pannen"
                    android:textColor="@color/black"
                    android:textSize="30dp" />
            </TableRow>
        </TableLayout>
    </ScrollView>

    <!-- Second rectangle (Black rectangle) -->

    <View
        android:id="@+id/rectangle8"
        android:layout_width="415dp"
        android:layout_height="106dp"
        android:layout_marginTop="16dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />



    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Ons assortiment"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.703"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115" />

    <view
        android:id="@+id/view6"
        class="android.widget.ActionMenuView"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_centerHorizontal="true"
        android:background="#000000"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/title"
        app:layout_constraintVertical_bias="0.182" />



    <ImageButton
        android:id="@+id/witmenuhome2"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:srcCompat="@drawable/icons8_restaurant_menu_64__1_" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="112dp"
        android:layout_height="96dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.899"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.998"
        app:srcCompat="@drawable/icons8_shopping_cart_52__2_" />

    <ImageButton
        android:id="@+id/withuisjemenu"
        android:layout_width="112dp"
        android:layout_height="96dp"
        android:backgroundTint="#000000"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.998"
        app:srcCompat="@drawable/icons8_home_32__2_" />

    <TextView
        android:id="@+id/add_alarm_action_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="181dp" />

    <!-- Floating action button for add person -->
    <!-- Make sure that you are constraining this
         button to the add Alarm FAB button -->

    <!-- Action name text for the add person button -->
    <!-- Make sure that you are constraining this Text
         to the add Person FAB button -->

    <TextView
        android:id="@+id/add_person_action_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="109dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
